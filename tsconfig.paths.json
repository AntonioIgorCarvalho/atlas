{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@data/*": ["src/data/*"],
      "@store/*": ["src/store/*"],
      "@config/*": ["src/config/*"],
